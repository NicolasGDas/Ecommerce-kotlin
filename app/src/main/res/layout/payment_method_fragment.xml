<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.PaymentMethodFragment">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonContinue2"
        android:layout_width="356dp"
        android:layout_height="55dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="480dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_corner"
        android:backgroundTint="#42BF65"
        android:text="Continuar"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/TransferenciaTextView"
        android:layout_width="278dp"
        android:layout_height="72dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="388dp"
        android:layout_marginEnd="60dp"
        android:background="#95AFF1"
        android:gravity="center"
        android:text="Transferencia"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.307"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="158dp"
        android:layout_height="45dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="136dp"
        android:text="Metodos de pago"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mercadoPagoTextView"
        android:layout_width="278dp"
        android:layout_height="72dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="196dp"
        android:layout_marginEnd="60dp"
        android:background="#95AFF1"
        android:gravity="center"
        android:text="Mercado Pago"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.307"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/EfectivoTextView"
        android:layout_width="278dp"
        android:layout_height="72dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="296dp"
        android:layout_marginEnd="60dp"
        android:background="#95AFF1"
        android:gravity="center"
        android:text="Efectivo"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.307"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="68dp"
        android:layout_height="45dp"
        android:layout_marginStart="12dp"
        android:src="@drawable/mercado_libre_logo"
        app:layout_constraintBottom_toBottomOf="@+id/mercadoPagoTextView"
        app:layout_constraintStart_toStartOf="@+id/mercadoPagoTextView"
        app:layout_constraintTop_toTopOf="@+id/mercadoPagoTextView" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="76dp"
        android:layout_height="51dp"
        android:layout_marginStart="12dp"
        android:src="@drawable/money_logo"
        app:layout_constraintBottom_toBottomOf="@+id/EfectivoTextView"
        app:layout_constraintStart_toStartOf="@+id/EfectivoTextView"
        app:layout_constraintTop_toTopOf="@+id/EfectivoTextView" />

</androidx.constraintlayout.widget.ConstraintLayout>